{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "dobuuk/availability",
  "version": "1.1.0",
  "title": "DoBu.uk Availability",
  "category": "embed",
  "icon": "calendar-alt",
  "description": "Embed an availability calendar and enquiry form for a DoBu.uk user",
  "supports": {
    "html": false
  },
  "textdomain": "dobuuk-availability",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "nickname": {
      "type": "string",
      "default": "test"
    }
  },
  "keywords": [
    "calendar",
    "availability",
    "booking",
    "enquiry"
  ]
}